<mimeo_configuration>
    <vars>
        <CUSTOM_VAR_1>custom-value-1</CUSTOM_VAR_1>
        <CUSTOM_VAR_2>1</CUSTOM_VAR_2>
        <CUSTOM_VAR_3>true</CUSTOM_VAR_3>
        <CUSTOM_VAR_4>{CUSTOM_VAR_2}</CUSTOM_VAR_4>
        <CUSTOM_VAR_5>{auto_increment}</CUSTOM_VAR_5>
