<mimeo_configuration>
    <_templates_>
        <_template_>
            <count>30</count>
            <model>

                <SomeEntity>
                    <TomorrowAtTheSameTime>
                        <_mimeo_util>
                            <_name>date_time</_name>
                            <days_delta>1</days_delta>
                        </_mimeo_util>
                    </TomorrowAtTheSameTime>
                    <TomorrowThreeHoursLater>
                        <_mimeo_util>
                            <_name>date_time</_name>
                            <days_delta>1</days_delta>
                            <hours_delta>3</hours_delta>
                        </_mimeo_util>
                    </TomorrowThreeHoursLater>
                    <TomorrowThreeHoursLaterTwentyMinutesAgo>
                        <_mimeo_util>
                            <_name>date_time</_name>
                            <days_delta>1</days_delta>
                            <hours_delta>3</hours_delta>
                            <minutes_delta>-20</minutes_delta>
                        </_mimeo_util>
                    </TomorrowThreeHoursLaterTwentyMinutesAgo>
                    <TomorrowThreeHoursLaterTwentyMinutesAgoTwoSecondsLater>
                        <_mimeo_util>
                            <_name>date_time</_name>
                            <days_delta>1</days_delta>
                            <hours_delta>3</hours_delta>
                            <minutes_delta>-20</minutes_delta>
                            <seconds_delta>2</seconds_delta>
                        </_mimeo_util>
                    </TomorrowThreeHoursLaterTwentyMinutesAgoTwoSecondsLater>
                    <OneHourAgo>
                        <_mimeo_util>
                            <_name>date_time</_name>
                            <hours_delta>-1</hours_delta>
                        </_mimeo_util>
                    </OneHourAgo>
                    <OneMinuteAgo>
                        <_mimeo_util>
                            <_name>date_time</_name>
                            <minutes_delta>-1</minutes_delta>
                        </_mimeo_util>
                    </OneMinuteAgo>
                    <OneSecondAgo>
                        <_mimeo_util>
                            <_name>date_time</_name>
                            <seconds_delta>-1</seconds_delta>
                        </_mimeo_util>
                    </OneSecondAgo>
                </SomeEntity>

            </model>
        </_template_>
    </_templates_>
</mimeo_configuration>